        function funPriceTotal() {
            // Total Debit
            var Total = 0;
            $('.txtTotalPrice').each(function () {

                // Get Value
                var Price = $(this).text();
                // Check Value
                if (Price == '' || Price == null || Price == undefined) {
                    Price = 0;
                }
                // Add Value
                Total += parseFloat(Price);

                return Total
            })

            // SET Values
            $('.txtVouchPrice').text(Total.toFixed(2));

        }
        function funVatTotal() {

