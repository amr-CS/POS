            },
            function (data, status) {
                if (data != undefined && data != '') {
                    var vBody = $('.tblItemBody')
                    vBody.html('')
                    var DataResult = JSON.parse(data)
                    $.each(DataResult, function (i, Item) {
                        vBody.append(funConfigHead(Item));
                    })

                }
            })
    }
