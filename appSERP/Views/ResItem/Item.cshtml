            var Account = vElement.find('.txtAccount').val();
            // Check If The Row Changed
            if (vElement.attr('data-changed') == '1') {

                    if (vItemCode == null || vItemCode.length == 0 || vItemCode.trim() == '0') {
                        funNotification("يجب إدخال كود الصنف لا يقبل الفراغ", 2);
                        vElement.find('.txtItemCode').focus();
                        return valid = false;
                    }

                // هنا الاستدعاء لل ابي غاي بواسطة جيكوري تحديث قيمة المتغير لن تتم حاولت
                // بأكثر من حل لكن لا تتوفر في الجاف سكريبت والتي متوفر لا يحل المشكلة
                if (vItemNameL1 != '') { // أثناء الاضافة والتعديل
                    $.get('/api/APIInvItem/InvItemGET',
                        {
                            pInvItemId: vItemId,
                            pInvItemNameL1: vItemNameL1,
                            pInvItemNameL2: vItemNameL2,
                            pInvItemCode: vItemCode,

                            pGRPId: vGroupId,
