</div>
<!-- Data Modal -->
<div id="dataModal" class="fade modal" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content" id="dataModalContent"></div>
    </div>
</div>


<!-- Cash Desk Scripts -->
<!-- Cash Desk -->
<script>

    // On Load
    $(document).ready(function () {

        // Drop Down  Font Size
        //funFillSizeType(null, 0, null, null,null, null, 1, null, null, false);

        // Wait 2 Seconds
        setTimeout( function () {
            // GET Font Size By Selected List
            funGETSizeTypeBySelectedList();
        },1000);
    })



    function funSaveType() {

        
        // Is Valid
        var vIsValid = true;

        // Check All Rows
        $(".tblSizeTypeBody > tr").each(function () {

            // Row Data
            var vRowData = $(this);
            // Get SizeType Id
            var vSizeTypeId = vRowData.attr('data-id');
            // Code
            var vSizeTypeCode = vRowData.find('.SizeTypeCode').val();
            // Name L1
            var vSizeTypeNameL1 = vRowData.find('.SizeTypeNameL1').val();
            // Name L2
            var vSizeTypeNameL2 = vRowData.find('.SizeTypeNameL2').val();
            // Is Active
            var vSizeTypeIsActive = vRowData.find('.SizeTypeIsActive').prop('checked');

            // Check Cash Desk If Empty
            if (vSizeTypeNameL1 != '') {

                // Save
                $.post('/SizeType/SizeTypeGET',
                    {
                        pSizeTypeId: vSizeTypeId,
                        pSizeTypeCode: vSizeTypeCode,
                        pSizeTypeNameL1: vSizeTypeNameL1,
                        pSizeTypeNameL2: vSizeTypeNameL2,
                        pSizeTypeIsActive: vSizeTypeIsActive,
                        pIsDeleted: false,
                        pQueryTypeId: 100
                    },
                    function (data, status) {

                        //// Drop Down Main Cash Desk
                        //funFillSizeType(null, 0, null, null,null, null, 1, null, null, false);

                        // GET Cash Desk By Selected List
                        funGETSizeTypeBySelectedList();
                    });
            }
            else {

                // Not Valid
                vIsValid = false;

            } // End Check Cash Desk Data

        })

        if (!vIsValid) {

            // Notification - Field Required
            funNotification('@appResource.msgRequired', 2);
        }
        else {

            // Notification
             funNotification('@appResource.msgSave', '1');
        }
    }


        // Save - Click

    $('.btnSizeTypeSave').on('click', function () {
        funSaveType();

    }); // End of Save Click

    function funSaveSize() {
        
        // Is Valid
        var vIsValid = true;

        // Check All Rows
        $(".tblSizeBody > tr").each(function () {

            // Row Data
            var vRowData = $(this);
            // Get SizeType Id
            var vSizeTypeId = localStorage.getItem('vSizeTypeId');
            // Id
            var vSizeId = vRowData.attr('data-size-id');

            // Code
            var vSizeCode = vRowData.find('.SizeCode').val();
            // Name L1
            var vSizeNameL1 = vRowData.find('.SizeNameL1').val();
            // Name L2
            var vSizeNameL2 = vRowData.find('.SizeNameL2').val();
            // Is Active
            var vSizeIsActive = vRowData.find('.SizeIsActive').prop('checked');

            console.log(vSizeIsActive)
            // Check Cash Desk If Empty
            if (vSizeNameL1 != '') {

                // Save
                $.post('/SizeType/SizeGET',
                    {
                        pSizeId: vSizeId,
                        pSizeCode: vSizeCode,
                        pSizeNameL1: vSizeNameL1,
                        pSizeNameL2: vSizeNameL2,
                        pSizeIsActive: vSizeIsActive,
                        pSizeTypeId: vSizeTypeId,
                        pIsDetail: false,
                        pIsDeleted: false,
                        pQueryTypeId: 100
                    },
                    function (data, status) {
                        // GET Cash Desk By Selected List
                        funGETSizeTypeDTl(vSizeTypeId);

                    });
            }
            else {

                // Not Valid
                vIsValid = false;

            } // End Check Cash Desk Data

        })

        if (!vIsValid) {

            // Notification - Field Required
            funNotification('@appResource.msgRequired', 2);
        }
        else {

            // Notification
            funNotification('@appResource.msgSave', 1);
        }
    }
       // Save - Click

    $('.btnSizeSave').on('click', function () {
        funSaveSize()

    }); // End of Save Click
    $('.btnSave').on('click', function () {
        funSaveType();
        funSaveSize();

    })


    $('body').on('click', '.btnSizeTypeDelete', function () {
        // Get SizeType Id
        var vSizeTypeIdDelete = $(this).attr('data-id');
        // Save
        $.post('/SizeType/SizeTypeGET',
            {
                pSizeTypeId: vSizeTypeIdDelete,
                pSizeTypeCode: null,
                pSizeTypeNameL1: null,
                pSizeTypeNameL2: null,
                pSizeTypeIsActive: null,
                pIsDeleted: true,
                pQueryTypeId: 300
            },
            function (data, status) {

                //// Drop Down Main Cash Desk
                //funFillSizeType(null, 0, null, null, null, null, 1, null, null, false);

                // GET Cash Desk By Selected List
                funGETSizeTypeBySelectedList();
                funGETSizeTypeDTl(0);

            });

        // Notification
        funNotification('@appResource.msgDelete', 1);

    })
    
    $('body').on('click', '.btnSizeDelete', function () {
        // Get SizeType Id
        var vSizeTypeIdDelete = $(this).attr('data-id');
          // Get Size Id
        var vSizeIdDelete = $(this).attr('data-size-id');
        // Save
        $.post('/SizeType/SizeGET',
            {
                pSizeId: vSizeIdDelete,
                pSizeCode: null,
                pSizeNameL1: null,
                pSizeNameL2: null,
                pSizeIsActive: null,
                pIsDeleted: true,
                pQueryTypeId: 300
            },
            function (data, status) {
                var vSizeTypeId = localStorage.getItem('vSizeTypeId');
                // GET Cash Desk By Selected List
                funGETSizeTypeDTl(vSizeTypeId);

            });

        // Notification
              funNotification('@appResource.msgDelete', 1);
    })
    // Add New Row
    $('body').on('click', '.btnSizeTypeAddRow', function () {

        var vCurrentRow = $(this).closest('tr');
        // Cash Desk Add Row
        funSizeTypeAddRow(vCurrentRow);
    })

    // GET Cash Desk By Selected List
    function funGETSizeTypeBySelectedList() {

        // Get Value [Id]
        var vSelectedSizeType = $('#selectSizeTypeMain').val();

        // GET Data
        $.get('/SizeType/SizeTypeGET',
            {
                pSizeTypeId: null,
                pSizeTypeCode: '',
                pSizeTypeNameL1: '',
                pSizeTypeNameL2: '',
                pSizeTypeIsActive: null,
                pIsDeleted: false,
                pQueryTypeId: 400
            },
            function (SizeTypeData, status) {

                // Table Body
                var vSizeTypeTableBody = $('.tblSizeTypeBody');
                // HTML CLEAR
                vSizeTypeTableBody.html('');

                // JSON
                var vSizeTypeDataResult = JSON.parse(SizeTypeData);

                // For Each Cash Desk - Main
                $.each(vSizeTypeDataResult, function (m, SizeTypeSubData) {

                    // GET Cash Desk Row Content
                    var vSizeTypeRowContent = funSizeTypeRowConfig(vSizeTypeDataResult[m]);
                    // Append Row Content
                    vSizeTypeTableBody.append(vSizeTypeRowContent);

                })
                //funSizeTypeRow();
            }) }
    // Cash Desk Add Row
    function funSizeTypeAddRow(pCurrentRow) {
        // Row Content Data
        var vRowContentData =
        {
            "SizeTypeIsActive": true,
            "SizeTypeId": 0,
            "SizeTypeCode": '',
            "SizeTypeNameL1": '',
            "SizeTypeNameL2": '',
        }

        var vRowContent = funSizeTypeRowConfig(vRowContentData);
        // Add Row After Current Row
        $(pCurrentRow).closest("tr").after(vRowContent);
    }


    // Cash Desk Row Config
    function funSizeTypeRowConfig(pSizeTypeDataResult) {

        // Checked
        var vChecked = null;
        if (pSizeTypeDataResult.SizeTypeIsActive) {
            vChecked = 'checked';
        }

        // Type
        var vSelectedText1 = '';
        var vSelectedText2 = '';
        var vSizeTypeId = pSizeTypeDataResult.SizeTypeId;
        if (vSizeTypeId == 1) {

            vSelectedText1 = 'selected';
        }
        else {
            vSelectedText2 = 'selected';
        }

        // Row Content
        var vRowContent = '<tr class="tblRow" data-id="' + pSizeTypeDataResult.SizeTypeId + '">' +
            '<td width="20px"><button class="btn btn-sm btn-light btnSizeTypeAddRow text-secondary"><i class="fa fa-plus-circle"></i></button></td>' +
            '<td width="7%"><input type="text" class="form-control SizeTypeCode" disabled value="' + pSizeTypeDataResult.SizeTypeCode + '" /></td>' +
            '<td><input type="text" class="form-control SizeTypeNameL1" value="' + pSizeTypeDataResult.SizeTypeNameL1 + '" /></td>' +
            '<td><input type="text" class="form-control SizeTypeNameL2" value="' + pSizeTypeDataResult.SizeTypeNameL2 + '" /></td>' +
            '<td><input type="checkbox" class="form-control  SizeTypeIsActive" ' + vChecked + ' /></td>' +
            '<td><button class="btn btn-light btn-sm btnSizeTypeDelete" data-id="' + pSizeTypeDataResult.SizeTypeId + '"><i class="fa fa-trash"></i></button></td>' +
            '</tr>';

        // Return Row Content
        return vRowContent;
    }



    // Row Select
    $('.tblSizeTypeBody').on('click', '.tblRow', function () {

        $('.tblRow').removeClass('table-primary');
        $(this).addClass('table-primary');

    })

    // Get Id Of Main Group
    $('.divSizeType').on('click', 'table tr', function () {
        var vSizeTypeId = $(this).attr('data-id')
        localStorage.setItem('vSizeTypeId', vSizeTypeId);
        funGETSizeTypeDTl(vSizeTypeId)
    });


    // GET Cash Desk By Selected List
    function funGETSizeTypeDTl(pSizeType) {


        // GET Data
        $.get('/SizeType/SizeGET',
            {
                pSizeId: null,
                pSizeCode: '',
                pSizeNameL1: '',
                pSizeNameL2: '',
                pSizeTypeId: pSizeType,
                pSizeIsActive: null,
                pIsDetail: true,
                pQueryTypeId: 400
            },
            function (SizeTypeData, status) {

                // Table Body
                var vSizeTypeTableBody = $('.tblSizeBody');
                // HTML CLEAR
                vSizeTypeTableBody.html('');

                // JSON
                var vSizeTypeDataResult = JSON.parse(SizeTypeData);

                // For Each Cash Desk - Main
                $.each(vSizeTypeDataResult, function (m, SizeTypeSubData) {

                    // GET Cash Desk Row Content
                    var vSizeTypeRowContent = funSizeTypeDTlRowConfig(vSizeTypeDataResult[m]);
                    // Append Row Content
                    vSizeTypeTableBody.append(vSizeTypeRowContent);

                })

                // Cash Desk Add Row
                funAccountAddRow()
            })
    }



</script>
