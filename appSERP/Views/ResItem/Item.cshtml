                            //
                            pItemType: 2,
                            pInvItemIsActive: chkIsActive,
                            pIsDeleted: false,
                            pQueryTypeId: '100',
                            pPrinterId: vPrinterId,
                            pAccount: Account
                        }
                        ,function (data) {
                            var vResultData = JSON.parse(data) // pData != 'undefined' && pData != 'null'
                            if (vResultData[0].SuccessStatus == 'undefined' || vResultData[0].SuccessStatus == false) {
                                funNotification(vResultData[0].SystemMessageText, 3);
                                valid = false;
                                return valid = false;
                            }
                            vElement.attr('data-changed', '0');
                            if (vItemId == 0) {
                                vElement.find('.txtItemId').val(vResultData[0].InvItemId);
                                vElement.find('.txtItemCode').val(vResultData[0].InvItemCode);
                                var Valid33 = funSaveUnit(vResultData[0].InvItemId); // عند الإضافة

                            }
                            else {
                                var vUnitId = $('.txtHeadUnitId').val();

                                // Item Id
                                //var vItemId = $('.txtHeadItemId').val();

                                if (vItemId != 0) { // Save Price // عند التعديل يستدعى
                                    ValidItem = funSaveUnit(vItemId);
                                    SavePrice(vUnitId);
                                    //funNotification('@appResource.msgSave', 1);
                                  }
                            }


                            return valid = true;
                        });

                    return valid;

                    }

                else {

                    // Error Class Validation
                    if (vItemNameL1 == '') { vElement.find('.txtItemNameL1').addClass('errorClass') }
                    if (vCatAccount == '') { vElement.find('.txtCatAccountCode').addClass('errorClass') }
                    return valid = false;
                }
            }

        });
        return valid;
    }



