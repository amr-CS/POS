    // Save Unit
    function funSaveUnit(pItemId) {
        let Units = [];

        var vInvItemUnitId ;

        $('.tblUnitRow').each(function () {
            // Row Data
            var vRowData = $(this).closest('tr');

            // Save Items
             vInvItemUnitId = vRowData.attr('data-id');
            var vUnitId = vRowData.find('.txtUnitId').val();
            var vPartsInParents = vRowData.find('.txtPartsInParent').val();
            var vUnitParentId = vRowData.find('.txtUnitParentId').val();
            if (vPartsInParents === '') {
                vPartsInParents=1
            }
            var vItemId = $('.txtHeadItemId').val();

            var vBarcode = vRowData.find('.txtBarcode').val();
            var vIsDecreasable = vRowData.find('.txtIsDecreasable').prop('checked');
            var vSellUnit = vRowData.find('.chkSellUnit').prop('checked');
            var vUnitProduction = vRowData.find('.chkUnitProduction').prop('checked');
            var vIsDefault = vRowData.find('.chkIsDefault').prop('checked');
            var vUnitIsActive = vRowData.find('.chkIsActive').prop('checked');
           // if (vRowData.attr('data-changed') == '1') {
                if (vUnitId && parseInt(vUnitId) > 0 ) {
                    vRowData.attr('data-changed', '0')
                    Units.push({
                        InvItemUnitId: vInvItemUnitId,
                        UnitId: vUnitId,
                        PartsInParents: vPartsInParents,
                        UnitParentId: vUnitParentId,
                        ItemId: pItemId,
                        Barcode: vBarcode,
                        IsDecreasable: vIsDecreasable,
                        SellUnit: vSellUnit,
                        UnitProduction: vUnitProduction,
                        IsDecreasable: vIsDecreasable,
                        IsDefault: vIsDefault,
                        UnitIsActive: vUnitIsActive,
                        QueryTypeId: 100
                    });
                };
          //  }
        });
        console.log(Units)
        $.ajax({
            url: '/ResItem/InsertItemBulk',
            method: 'POST', contentType: 'application/json',
            data: JSON.stringify({
                Units,
                ItemId: pItemId
            }),
            beforeSend: function () {
                $('#loading').show();
            },
            complete: function () {
                $('#loading').hide();
            },
            success: function (data) {
                var vResultData = JSON.parse(data)
                if (vResultData[0].IsNew==1) {
                    //vRowData.attr('data-id', vResultData[0].UnitId);
                    $('.txtHeadUnitId').val(vResultData[0].UnitId)

                    SavePrice(vResultData[0].UnitId)
                }
                if (vResultData[0].SuccessStatus == 'undefined' || vResultData[0].SuccessStatus == false)
                    funNotification(vResultData[0].SystemMessageText, 3);
                else funNotification('@appResource.msgSave', 1);
                ////return vResultData[0].SuccessStatus;
            }
        });

    }

    // Table Save Item
