<!-- Size Type -->
<div class="divTableContainer">
    <!-- Utlity -->
    <div class="divUtilityBar d-flex ">
        @Html.Partial("~/Views/ViewSetting/ViewSettingUtilityBar.cshtml")
    </div>
    <!-- Cash Desk Header -->
    <div class="w-100 divSectionTitle p-2 px-4"> @appResource.lblSizeType</div>

    <!-- Cash Desks Data -->
    <div class="divSizeType bg-white border p-2">
